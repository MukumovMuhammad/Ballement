@onready var camera_2d = $Camera2D
var SceneLoader
var Cam_pos : Array = [Vector2(560, 325), Vector2(2600,325)] 

func _ready():
	
	camera_2d.global_position = Cam_pos[0]
func _on_levels_pressed():
	#get_tree().change_scene_to_file(\"res://Scenes/UI/Level_choise.tscn\")
	camera_2d.global_position = Cam_pos[1]


func _on_exit_pressed():
	camera_2d.global_position = Cam_pos[0]


func _on_train_pressed():
	Change_scene(\"res://Scenes/Levels/training.tscn\")


func _on_quit_pressed():
	get_tree().quit()



func Change_scene(scene : String):
	SceneLoader = preload(\"res://Scenes/UI/loading_scene.tscn\").instantiate()
	SceneLoader.SceneLoad = scene
	add_child(SceneLoader)
	
